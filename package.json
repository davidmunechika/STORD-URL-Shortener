    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mocha": "^8.3.2",
    "mongoose": "^5.12.1",
    "nodemon": "^2.0.7",
    "shortid": "^2.2.16",
    "supertest": "^6.1.3",
    "valid-url": "^1.0.9"
  }
}
