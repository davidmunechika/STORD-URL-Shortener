{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "STORD URL Shortener Coding Exercise",
  "main": "server.js",
  "scripts": {
    "test": "mocha --recursive --exit",
    "start": "concurrently \"npm run prod\" \"npm run client\"",
    "conc": "concurrently \"npm run dev\" \"npm run client\"",
